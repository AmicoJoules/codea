{
  "name": "codea",
  "version": "1.0.0",
  "description": "Te encanta compartir conocimiento a través de proyectos de código abierto, libros etc. Hemos decidido crear este proyecto para cualquier persona que quiera colaborar.",
  "main": "gatsby-browser.js",
  "author": "Jorge Luis Calleja Alvarado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codea-team/codea/issues"
  },
  "homepage": "https://github.com/codea-team/codea#readme",
  "keywords": [
    "codea",
    "react",
    "static-site",
    "gatsby",
    "blog",
    "docs",
    "javaScript"
  ],
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codea-team/codea.git"
  },
  "scripts": {
    "start": "gatsby serve",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint -- .",
    "lint:staged": "lint-staged",
    "prettier": "prettier --ignore-path \"./.prettierignore\" --config .prettierrc --write \"./src/**/*.js\""
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": [
    "prettier",
    "lint:staged"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.45",
    "gatsby-image": "^2.0.19",
    "gatsby-paginate": "^1.0.16",
    "gatsby-plugin-catch-links": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.1",
    "gatsby-plugin-sass": "^2.0.3",
    "gatsby-plugin-sharp": "^2.0.12",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-remark-copy-linked-files": "^2.0.6",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.0.3",
    "gatsby-remark-responsive-iframe": "^2.0.6",
    "gatsby-remark-smartypants": "^2.0.6",
    "gatsby-source-filesystem": "^2.0.7",
    "gatsby-transformer-remark": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.8",
    "gatsby-transformer-yaml": "^2.1.4",
    "jss": "^9.8.7",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.10.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.0"
  }
